	"scripts": {
		"dev": "craco start",
		"start": "./node_modules/.bin/serve -s build -l 4005",
		"build": "craco build",
		"eject": "craco eject",
		"test": "craco test --watchAll=false",
		"test:watch": "npm test -- --watch",
		"test:coverage": "npm test -- --coverage --watchAll=false",
		"test:open-coverage": "open ./coverage/lcov-report/index.html",
		"lint:check": "eslint . --ext .js,.jsx,.ts,.tsx",
		"lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
		"stylelint:check": "stylelint 'src/**/*.styles.ts'",
		"stylelint:fix": "stylelint 'src/**/*.styles.ts' --fix",
		"prettier:check": "prettier --check 'src/**/*.{js,jsx,ts,tsx,json}'",
		"prettier:format": "prettier --write 'src/**/*.{js,jsx,ts,tsx,json}'",
		"prebuild": "npm run lint:check && npm run stylelint:check && rm -rf build",
		"prepare": "husky"
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"devDependencies": {
		"@babel/plugin-proposal-private-property-in-object": "^7.21.11",
		"@craco/craco": "^7.1.0",
